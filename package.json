{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "prepare-commit-msg": "node scripts/prepare-commit-message.js",
      "commit-msg": "node scripts/lint-commit-message.js",
      "pre-commit": "node scripts/warn-npm-install.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuetifyjs/vuetify.git"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "start": "lerna run start --scope vuetifyjs.com --stream",
    "lint": "lerna run lint --parallel --stream",
    "lint:fix": "lerna run lint:fix --parallel",
    "version": "node scripts/confirm-npm-tag.js",
    "prepare": "node scripts/post-install.js",
    "postversion": "node scripts/post-release-merge.js",
    "clean": "lerna clean",
    "changelog": "conventional-changelog -u -p vuetify",
    "all-checks": "yarn lint && lerna run test && lerna run cy:run && yarn build"
  },
  "engines": {
    "node": ">=18.19.0 || >=20.6.0",
    "yarn": "^1.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@mdi/font": "6.2.95",
    "@mdi/js": "6.2.95",
    "@mdi/svg": "6.2.95",
    "@octokit/core": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unhead/vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.4.27",
    "@vueuse/head": "^1.3.1",
    "alpinejs": "^3.14.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "conventional-changelog-cli": "^3.0.0",
    "conventional-changelog-vuetify": "^1.2.1",
    "conventional-github-releaser": "^3.1.5",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-local-rules": "^2.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-vue": "^9.24.1",
    "eslint-plugin-vuetify": "^2.3.0",
    "husky": "^3.1.0",
    "inquirer": "^6.5.2",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-serializer-html": "^7.1.0",
    "lerna": "^3.22.1",
    "magic-string": "^0.30.9",
    "mkdirp": "^3.0.1",
    "moment": "^2.30.1",
    "patch-package": "^8.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.74.1",
    "semver": "^7.6.0",
    "shelljs": "^0.8.5",
    "typescript": "~5.3.3",
    "upath": "^2.0.1",
    "vite-plugin-inspect": "^0.8.3",
    "vue": "^3.4.27",
    "vue-analytics": "^5.16.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27",
    "yargs": "^17.7.2"
  }
}
